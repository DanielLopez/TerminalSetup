sh ~/TerminalSetup/scripts/pre-sync

echo "Syncing ....."
mkdir -p ~/TerminalSetup/.config/nvim
mkdir -p ~/TerminalSetup/.tmux

rsync -rL ~/.config/nvim ~/TerminalSetup/.config/
rsync -rL ~/.tmux/ ~/TerminalSetup/.tmux/
rsync -L ~/.tmux.conf ~/TerminalSetup/
rsync -L ~/.bashrc ~/TerminalSetup/
echo "Sync Completed"
